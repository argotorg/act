contract C

constructor(uint24 _x)

creates

  uint24 x := _x

transition increase(uint24 new_x) : uint24

  updates
    x := x

  returns pre(x)

ensures

  post(x) >= pre(x)

// Second contract, with same behaviour name
contract D

