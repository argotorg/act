contract A

constructor()

creates
  uint x := 0


contract B

constructor(address<A> pa1, address p2, address<A> pa3, address p4)

iff
  pa1 != pa3

creates
  A a := pa1
  uint y := 0
  address bu1 := p2
  address bu2 := p4


transition f(address<A> pa1)

iff
  pa1 != a

updates
  a := new A()


contract C

constructor(address<A> pa1, address<A> pa2)

iff
  pa1 != pa2

creates
  B b1 := new B(pa1, pa2, new A(), new A())
  A a1 := new A()
  address cu1 := new A()
  address cu2 := new A()


transition f(address<A> pa1, address<A> pa2)

iff
  pa1 != pa2
  pa1 != a1
  pa2 != a1
  pa1 != b1
  pa2 != b1
  pa1 != b1.a
  pa2 != b1.a

updates
  b1 := new B(pa1, pa2, new A(), new A())
  b1.y := 42
  b1.a := new A()
  b1.a.x := 17
  



