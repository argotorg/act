contract Homogeneous

constructor()

creates
	uint256 x := 3
	uint256 y := 5
	uint256 z := 15

invariants
	x * y == z


transition f(uint256 scalar)

iff in range uint256
	x * scalar
	z * scalar

updates
	x := x * scalar
	z := z * scalar


transition g(uint256 scalar)

iff in range uint256
	y * scalar
	z * scalar

updates
	y := y * scalar
	z := z * scalar
