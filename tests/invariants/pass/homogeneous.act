contract Homogeneous

constructor
interface constructor()

creates
	uint256 x := 3
	uint256 y := 5
	uint256 z := 15

invariants
	x * y == z


transition f
interface f(uint256 scalar)

iff in range uint256
	x * scalar
	z * scalar

storage
	x := x * scalar
	z := z * scalar


transition g
interface g(uint256 scalar)

iff in range uint256
	y * scalar
	z * scalar

storage
	y := y * scalar
	z := z * scalar
