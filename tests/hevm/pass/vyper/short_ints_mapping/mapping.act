contract A

constructor()
iff
  CALLVALUE == 0

creates
  mapping(uint64 => mapping(uint128 => uint32)) m := []


transition setm(uint64 a, uint128 b, uint32 c)

iff
  CALLVALUE == 0

updates
  //m[a][b] := c
  m := m [a => [b=>c]]


transition m(uint64 idx1, uint128 idx2) : uint32

iff CALLVALUE == 0

updates
returns m[idx1][idx2]
