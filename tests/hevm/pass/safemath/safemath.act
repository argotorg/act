contract SafeAdd

transition add
interface add(uint256 x, uint256 y)

iff in range uint256

    x + y

iff
    CALLVALUE == 0

returns x + y

transition mul
interface mul(uint256 x, uint256 y)

iff in range uint256

    x * y

iff

    CALLVALUE == 0

returns x * y

transition double
interface double(uint256 x)

iff in range uint256

    2 * x

iff

    CALLVALUE == 0

returns 2 * x