contract SafeAdd

transition add(uint256 x, uint256 y) : uint256

iff in range uint256

    x + y

iff
    CALLVALUE == 0

returns x + y

transition mul(uint256 x, uint256 y) : uint256

iff in range uint256

    x * y

iff

    CALLVALUE == 0

returns x * y

transition double(uint256 x) : uint256

iff in range uint256

    2 * x

iff

    CALLVALUE == 0

returns 2 * x