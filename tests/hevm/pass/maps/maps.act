contract Map

constructor()

iff

  CALLVALUE == 0

creates
  uint256 val := 0
  mapping(uint => uint) f := [0 => 42]

transition val() : uint256

iff

  CALLVALUE == 0

returns pre(val)

transition f(uint x) : uint256

iff

  CALLVALUE == 0

returns pre(f[x])


transition set(uint value, uint key) : uint256

iff

  CALLVALUE == 0

storage

  f[key] := value

returns 1