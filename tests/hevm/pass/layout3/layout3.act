contract Map

constructor()

iff

  CALLVALUE == 0

creates
  uint128 val := 11
  mapping(uint => uint128) f := [11 => 42]

transition val() : uint128

iff

  CALLVALUE == 0

returns pre(val)

transition f(uint x) : uint128

iff

  CALLVALUE == 0

returns pre(f[x])


transition set(uint128 value, uint key) : uint256

iff

  CALLVALUE == 0

storage

  f[key] := value

returns 1