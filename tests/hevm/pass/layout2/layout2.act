iff

  CALLVALUE == 0

creates

  uint128 x := 11
  bool flag := true
  uint32 u := 17
  uint8 z := 3
  uint128 y := 42
  uint256 i := 128

transition foo() : uint256

iff

  CALLVALUE == 0

updates

  x := y
  y := x
  z := 11
  flag := not flag

returns 1

transition set_flag(bool b)

iff

  CALLVALUE == 0

updates

  flag := b


transition get_flag() : bool

iff

  CALLVALUE == 0

updates
