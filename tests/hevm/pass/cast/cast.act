contract A

constructor(uint x0)

creates
    uint x := x0


transition getx() : uint256

updates
returns x


transition setx(uint x1)

updates
  x := x1


contract B

constructor(address<A> x, address<A> y)

iff
    x != y

creates
    A a1 := x
    A a2 := y


transition upd()

updates
  a1.x := 42
  a2.x := 11
